};
typedef bool equalsFuncType(void *key, void *key0);
bool map_contains_key(struct node *map, void *key, equalsFuncType *equalsFunc)
{
if (map == 0)
return false;
else {
bool eq = equalsFunc(map->key, key);
if (eq)
