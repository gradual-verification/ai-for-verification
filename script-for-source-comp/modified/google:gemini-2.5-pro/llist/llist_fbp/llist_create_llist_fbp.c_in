struct node {
struct node *next;
int value;
};
struct llist {
struct node *first;
struct node *last;
};
struct llist *create_llist()
{
struct llist *l = malloc(sizeof(struct llist));
if (l == 0) abort();
struct node *n = calloc(1, sizeof(struct node));
if (n == 0) abort();
l->first = n;
l->last = n;
return l;
}
